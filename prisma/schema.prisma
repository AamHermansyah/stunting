generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model article {
  id         Int      @id @default(autoincrement())
  title      String
  summary    String
  category   String
  tags       String
  content    String   @db.LongText
  image      Bytes
  alt_image  String
  created_at DateTime @default(now())
}
